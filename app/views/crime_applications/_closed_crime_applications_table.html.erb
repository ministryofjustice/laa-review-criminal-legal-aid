<table class="govuk-table app-dashboard-table">
  <thead class="govuk-table__head">
    <tr class="govuk-table__row">
      <th scope="col" class="govuk-table__header">
        <%= t('.table_headings.applicant_name') %>
      </th>
      <th scope="col" class="govuk-table__header">
        <%= t('.table_headings.reference') %>
      </th>
      <th scope="col" class="govuk-table__header">
        <%= t('.table_headings.received_on') %>
      </th>
      <th scope="col" class="govuk-table__header">
        <%= t('.table_headings.reviewed_on') %>
      </th>
      <th scope="col" class="govuk-table__header">
        <%= t('.table_headings.reviewer') %>
      </th>
      <th scope="col" class="govuk-table__header">
        <%= t('.table_headings.status') %>
      </th>
    </tr>
  </thead>
  <tbody class="govuk-table__body">
    <%= render partial: 'closed_crime_applications_table_body',
               collection: applications,
               as: :app %>
  </tbody>
</table>
