<tr class="govuk-table__row">
  <th scope="row" class="govuk-table__header">
    <%= link_to app.applicant_name,
                application_start_path(app),
                class: 'govuk-link--no-visited-state govuk-!-font-weight-regular' %>
  </th>
  <td class="govuk-table__cell">
    <%= app.reference %>
  </td>
  <td class="govuk-table__cell">
    <%= app.application_type.humanize %>
  </td>
  <td class="govuk-table__cell">
    <%= l(app.submitted_at) %>
  </td>
  <td class="govuk-table__cell">
    <%= t('values.days_passed', count: app.days_passed) %>
  </td>
  <td class="govuk-table__cell">
    <%= app.assignee_name %>
  </td>
</tr>
