<%= govuk_table do |table| %>
  <%= table.with_head do |head|
        head.with_row do |row|
          row.with_cell(text: thead_text(:application_type), header: true)
          row.with_cell(text: thead_text(:received), numeric: true, header: true)
          row.with_cell(text: thead_text(:closed), numeric: true, header: true)
        end
      end %>
  <%= table.with_body do |body|
        report.rows.each do |data_row|
          body.with_row do |row|
            row.with_cell(
              text: 'CAT 3'
            )
            row.with_cell(
              text: data_row.fetch(:received),
              numeric: true
            )
            row.with_cell(
              text: data_row.fetch(:closed),
              numeric: true
            )
          end
        end
      end %>
  <% end %>
